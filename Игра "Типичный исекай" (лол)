def lok4():
    global Hp
    global Mp
    global damage_min
    global damage_max
    global nastroenie
    global c2
    def attack2():
        global Mp
        global Hp
        global nastroenie
        b3 = 0
        deistvie = 0
        Hp_vrag = 60
        while b3 == 0:
            deistvie = deistvie + 3
            int(Hp)
            int(Hp_vrag)
            b2 = 0
            while b2 == 0:
                if Hp_vrag <= 0:
                    print("Вы победили!")
                    b3 = 1
                    b2 = 1
                    nastroenie = nastroenie - 10
                elif Hp <= 0:
                    print("Вы проиграли!")
                    lok1()
                Mp = Mp + 10
                Hp = Hp - random.randint(10, 25)
                print("Ваш враг - Грязевой краб")
                print("Ваши очки действий(Xp):" + str(deistvie))
                print("Ваш Hp: " + str(Hp))
                print("Ваш Mp: " + str(Mp))
                print("Возможные действия:")
                print("1)Атака(-2Xp)")
                print("2)Исцеление(-2Xp/-25Mp/+30Hp)")
                print("3)Оценить врага(-1Xp/-10Mp)")
                print("4)Сбежать(-4Xp)")
                print("5)Закончить ход(+3Xp/+10Mp/+5Hp")
                b1 = input("-")
                if b1 == "1":
                    if deistvie < 3:
                        print("-------------------------------------------------------------------------------------------")
                        print("Недостаточно очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif deistvie >= 2:
                        damage = random.randint(damage_min, damage_max)
                        Hp_vrag = Hp_vrag - damage
                        deistvie = deistvie - 2
                        print("-------------------------------------------------------------------------------------------")
                        print("Вы нанесли врагу " + str(damage) + " урона")
                        print("-------------------------------------------------------------------------------------------")
                        nastroenie = nastroenie - 5
                if b1 == "2":
                    if Mp < 25 or deistvie < 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Нехватает Маны или Очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif Mp >= 25 or deistvie >= 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Исцеление")
                        print("-------------------------------------------------------------------------------------------")
                        Hp = Hp + 30
                        Mp = Mp - 25
                        deistvie = deistvie - 2
                    else:
                        print("-------------------------------------------------------------------------------------------")
                        print("Ошибка действия")
                        print("-------------------------------------------------------------------------------------------")
                if b1 == "3":
                    if deistvie < 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Нехватает очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif deistvie >= 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Враг - Грязевой краб")
                        print("Hp врага - " + Hp_vrag)
                        print("Атака врага - 10-25")
                        print("-------------------------------------------------------------------------------------------")
                        deistvie = deistvie - 1
                        Mp = Mp - 10
                if b1 == "4":
                    if deistvie < 4:
                        print("-------------------------------------------------------------------------------------------")
                        print("Не хватает очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif deistvie >= 4:
                        print("-------------------------------------------------------------------------------------------")
                        print("Бой окончен")
                        print("-------------------------------------------------------------------------------------------")
                        b2 = 1
                        b3 = 1
                        nastroenie = nastroenie + 10
                if b1 == "5":
                    print("-------------------------------------------------------------------------------------------")
                    print("Ход окончен")
                    print("-------------------------------------------------------------------------------------------")
                    b2 = 1
                elif b1 < "1" and b1 > "5":
                    print("Неправильный ввод")
            b2 = 1
    if c2 == "1":
        global d1
        print("-----------------------------------------------------------------------------------------------------------")
        print("                                              Акт 4                                                        ")
        print("                                         Путь через горы                                                   ")
        print("-----------------------------------------------------------------------------------------------------------")
        print("Я пошёл через горы, как мне и сказала та девушка")
        time.sleep(2)
