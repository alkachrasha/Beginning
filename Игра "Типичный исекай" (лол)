                        print("3)Оценить врага(-1Xp/-10Mp)")
                        print("4)Сбежать(-4Xp)")
                        print("5)Закончить ход(+3Xp/+10Mp/+5Hp")
                        b1 = input("-")
                        if b1 == "1":
                            if deistvie < 3:
                                print("-------------------------------------------------------------------------------------------")
                                print("Недостаточно очков действий")
                                print("-------------------------------------------------------------------------------------------")
                            elif deistvie >= 2:
                                damage = random.randint(damage_min, damage_max)
                                Hp_vrag = Hp_vrag - damage
                                deistvie = deistvie - 2
                                print("-------------------------------------------------------------------------------------------")
