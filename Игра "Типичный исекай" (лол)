def lok2():
    global Hp
    global Mp
    global damage_min
    global damage_max
    global nastroenie
    global b3
    global a9
    def attack():
        global Hp
        global Mp
        global damage_min
        global damage_max
        global nastroenie
        global b3
        global a9
        b3 = 0
        print("Подсказки:")
        print("После действия в скобках указаны затраты на их выполнение")
        print("Если вы не хотите убивать монстра то можете попытаться сбежать")
        print("Если ваш уровень здоровья низок, используйте исцеление")
        deistvie = 0
        Hp_vrag = 100
        while b3 == 0:
            deistvie = deistvie + 3
            Mp = Mp + 10
            Hp = Hp + 5
            int(Hp)
            int(Hp_vrag)
            b2 = 0
            if Hp_vrag <= 0:
                print("Вы победили!")
                b3 = 1
                lok3()
            elif Hp <= 0:
                print("Вы проиграли!")
                lok1()
            while b2 == 0:
                if Hp_vrag < 0:
                    print("Враг погиб")
                    b2 = 0
                    b3 = 0
                    nastroenie = nastroenie - 10
                print("Ваш враг - Волк")
                print("Ваши очки действий(Xp):" + str(deistvie))
                print("Ваш Hp: " + str(Hp))
                print("Ваш Mp: " + str(Mp))
                print("Возможные действия:")
                print("1)Атака(-2Xp)")
                print("2)Исцеление(-2Xp/-25Mp/+30Hp)")
                print("3)Оценить врага(-1Xp/-10Mp)")
                print("4)Сбежать(-4Xp)")
                print("5)Закончить ход(+3Xp/+10Mp/+5Hp")
                b1 = input("-")
                if b1 == "1":
                    if deistvie < 3:
                        print("-------------------------------------------------------------------------------------------")
                        print("Недостаточно очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif deistvie >= 2:
                        damage = random.randint(damage_min, damage_max)
                        Hp_vrag = Hp_vrag - damage
                        deistvie = deistvie - 2
                        print("-------------------------------------------------------------------------------------------")
                        print("Вы нанесли врагу " + str(damage) + " урона")
                        print("-------------------------------------------------------------------------------------------")
                        nastroenie = nastroenie - 5
                if b1 == "2":
                    if Mp < 25 or deistvie < 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Нехватает Маны или Очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif Mp >= 25 or deistvie >= 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Исцеление")
                        print("-------------------------------------------------------------------------------------------")
                        Hp = Hp + 30
                        Mp = Mp - 25
                        deistvie = deistvie - 2
                    else:
                        print("-------------------------------------------------------------------------------------------")
                        print("Ошибка действия")
                        print("-------------------------------------------------------------------------------------------")
                if b1 == "3":
                    if deistvie < 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Нехватает очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif deistvie >= 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Враг - Волк")
                        print("Hp врага - " + str(Hp_vrag))
                        print("Атака врага - 35-50")
                        print("-------------------------------------------------------------------------------------------")
                        deistvie = deistvie - 1
                        Mp = Mp - 10
                if b1 == "4":
                    if deistvie < 4:
                        print("-------------------------------------------------------------------------------------------")
                        print("Не хватает очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif deistvie >= 4:
                        print("-------------------------------------------------------------------------------------------")
                        print("Бой окончен")
                        print("-------------------------------------------------------------------------------------------")
                        b3 = 1
                        nastroenie = nastroenie + 10
                        lok3()
                if b1 == "5":
                    b2 = 1
                    Hp = Hp - random.randint(35, 50)
                    print("-------------------------------------------------------------------------------------------")
                    print("Ход окончен")
                    print("-------------------------------------------------------------------------------------------")
                else:
                    print("")
            b2 = 1
    print("-----------------------------------------------------------------------------------------------------------")
    print("                                              Акт 2                                                        ")
    print("                                            Новый мир                                                      ")
    print("-----------------------------------------------------------------------------------------------------------")
    dialog1 = ["Я очутился на пустом берегу", "В далеке, в стороне моря, до самого горизонта нету ничего интересного", "Сам пляж пуст, тут тоже нет ничего интересного", "В далеке, в стороне леса, виден еле заметный дымок", "Чёртово существо, но делать нечего", "Стоит собрать больше информации", "Надо идти на дым"]
    b3 = 0
    while b3 < 7:
        print(dialog1[b3])
        time.sleep(2)
        b3 = b3 + 1
    print("В кустах что-то шуршит, враг?")
    if nastroenie < 999:
        attack()
    print("Ну и жуткая тварь, ну ничего, продолжу путь к дыму")
    lok3()
