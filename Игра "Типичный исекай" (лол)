                        print("-------------------------------------------------------------------------------------------")
                        print("Исцеление")
                        print("-------------------------------------------------------------------------------------------")
                        Hp = Hp + 30
                        Mp = Mp - 25
                        deistvie = deistvie - 2
                    else:
                        print("-------------------------------------------------------------------------------------------")
                        print("Ошибка действия")
                        print("-------------------------------------------------------------------------------------------")
                if b1 == "3":
                    if deistvie < 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Нехватает очков действий")
                        print("-------------------------------------------------------------------------------------------")
                    elif deistvie >= 1:
                        print("-------------------------------------------------------------------------------------------")
                        print("Враг - Князь тьмы")
                        print("Hp врага - " + str(Hp_boss))
                        print("Атака врага - 15-45")
                        deistvie = deistvie - 1
                        Mp = Mp - 10
                        print("-------------------------------------------------------------------------------------------")
                if b1 == "4":
                    if hod == 15:
                        if nastroenie > -30:
                            if deistvie < 4:
                                print("-------------------------------------------------------------------------------------------")
                                print("Не хватает очков действий")
                                print("-------------------------------------------------------------------------------------------")
                            elif deistvie >= 4:
                                print("-------------------------------------------------------------------------------------------")
                                print("Бой окончен")
                                print("-------------------------------------------------------------------------------------------")
                                b3 = 1
                                b2 = 1
                        elif nastroenie < -30:
                            print("")
                    else:
                        print("Бой нельзя закончить")
                if b1 == "5":
                    print("-------------------------------------------------------------------------------------------")
                    print("Ход окончен")
                    print("-------------------------------------------------------------------------------------------")
                    b2 = 1
                elif b1 > "5" and b1 < "1":
                    print("-------------------------------------------------------------------------------------------")
                    print("Неправильный ввод")
                    print("-------------------------------------------------------------------------------------------")
            b2 = 1
        if Hp_boss <= 0:
            n1 = 0
            dialog1 = ["Он мёртв", "Но я почему-то не рад", "Как будто, я стал пуст...", "Снова тебя приветствую странник", "Ты справился с поставленной задачей, я удивлён", "Многие до тебя не справлялись с этой задачей", "Оставить тут я тебя не смогу, но отправлю в лучший мир, что найду", "Это твоя награда..."]
            while n1 < 8:
                print(dialog1[n1])
                time.sleep(3)
                n1 = n1 + 1
        elif Hp_boss >= 1:
            n2 = 0
            dialog1 = ["Он сдался", "Я... рад, почему?", "Может мне надо закончить дело?", "Снова тебя приветствую странник", "Ты справился с задачей", "Остальное я могу уже сам сделать", "Я отправлю тебя в лучший мир, что найду", "Пожалуй, ты это заслужил"]
            while n2 < 8:
                print(dialog1[n2])
                time.sleep(3)
                n2 = n2 + 1
        time.sleep(2)
        exit()
lok1()
